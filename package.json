{
	"name": "simple-radar",
	"version": "1.0.0",
	"description": "A simple radar visualization with CSV data integration",
	"type": "module",
	"main": "src/be/server.ts",
	"scripts": {
		"be": "bun src/be/server.ts",
		"fe": "bun src/fe/serve.ts",
		"dev": "bun src/dev.ts",
		"prod": "bun src/prod.ts",
		"start": "bun src/dev.ts",
		"test": "bun test",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"format:fix": "biome format --write .",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"biome:all": "biome check --write . && biome format --write . && biome lint --write ."
	},
	"keywords": [
		"radar",
		"visualization",
		"bun",
		"csv"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@types/bun": "^1.3.1",
		"typescript": "^5.9.3"
	}
}
